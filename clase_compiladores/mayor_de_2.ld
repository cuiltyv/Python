# programa para obtener el mayor entre 2 numeros

program numero_mayor;

var num1,num2 : int; # variables donde se guardan valores

main {

    num1 = 3
    num2 = 10

    if (num1 > num2){
        print("El mayor numero es num1)
    }
    else{
        print("El mayor numero es num2)
    }


}
end 
# final de programa