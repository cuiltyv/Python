program celcius_to_farenheit;

var celcius, farenheit : float;

main {

    celcius = 10.0

    farenheit = (celcius * 9 / 5) + 32;

    print(farenheit);

    if(farenheit > 70){
        print("Calido");
    } else {
        print("Fresco)
    }

}
end

# El programa te da el farenheit, dependiendo del celcius de input


